#include <iostream>

double multiplyByFive(double& number) {
    number = number * 5;
    std::cout << "Number in function is = " << number << "\n";
    return number;
}

int main()
{
    double number = 0;
    std::cout << "Please input a numerical value : " << "\n";
    std::cin >> number;
    std::cout << "Number in main = " << number << "\n";
    multiplyByFive(number);
    std::cout << "Number in main post return is = " << number << "\n";
}
